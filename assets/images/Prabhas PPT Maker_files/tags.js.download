// ============== ASSET TRACKING SYSTEM ==============
// Tracks usage to prevent image/icon repetition

// Global tracking state - reset before each PPT generation
let usedImages = new Set();
let usedRandomImages = new Set(); // Track random-tagged images separately
let iconUsageCount = new Map();

/**
 * Reset all tracking - call this before generating a new PPT
 */
function resetAssetTracking() {
  usedImages.clear();
  usedRandomImages.clear(); // Also clear random images tracking
  iconUsageCount.clear();
  // Also reset colors in templates.js
  if (window.resetColors) {
    window.resetColors();
  }
}

/**
 * Check if an image has "random" tag
 * @param {object} img - Image object
 * @returns {boolean} - True if image has "random" tag
 */
function hasRandomTag(img) {
  if (!img.tags || img.tags.length === 0) return false;
  return img.tags.some(tag => tag.toLowerCase() === "random");
}

/**
 * Get a random-tagged image that hasn't been used yet
 * @param {array} images - All images
 * @returns {object|null} - Random-tagged image or null if none available
 */
function getRandomTaggedImage(images) {
  // Filter to get only images with "random" tag that haven't been used
  const randomTaggedImages = images.filter(img =>
    hasRandomTag(img) && !usedRandomImages.has(img.label) && !usedImages.has(img.label)
  );

  if (randomTaggedImages.length === 0) return null;

  // Pick a random one from available random-tagged images
  const selectedImage = randomTaggedImages[Math.floor(Math.random() * randomTaggedImages.length)];

  // Mark as used in both sets
  usedImages.add(selectedImage.label);
  usedRandomImages.add(selectedImage.label);

  return selectedImage;
}

/**
 * Get a background image based on keywords
 * Each image can only be used ONCE per entire PPT
 * When no match found, uses images with "random" tag (each random image used only once)
 * @param {string[]} keywords - Keywords to match
 * @returns {object|null} - Image object with path, or null if none available
 */
function getBackgroundImage(keywords) {
  // Filter to only images
  const images = tagData.filter(item => item.type === "image");

  if (!keywords || keywords.length === 0) {
    // No keywords provided - use random-tagged image
    const randomImage = getRandomTaggedImage(images);
    if (randomImage) return randomImage;

    // If no random-tagged images available, fall back to any unused image
    const unusedImages = images.filter(img => !usedImages.has(img.label));
    if (unusedImages.length === 0) return null;

    const selectedImage = unusedImages[Math.floor(Math.random() * unusedImages.length)];
    usedImages.add(selectedImage.label);
    return selectedImage;
  }

  // Normalize keywords
  const normalizedKeywords = keywords.map(k => k.toLowerCase().trim());

  // Score each image by matching tags (exclude "random" tag from scoring)
  const scoredImages = images
    .filter(img => !usedImages.has(img.label)) // Only unused images
    .map(img => {
      let score = 0;
      // Filter out "random" tag from scoring - we don't want random-tagged images to match
      const imageTags = img.tags
        .filter(t => t.toLowerCase() !== "random")
        .map(t => t.toLowerCase());

      normalizedKeywords.forEach(keyword => {
        imageTags.forEach(tag => {
          if (tag.includes(keyword) || keyword.includes(tag)) {
            score += 2;
          }
          // Partial match
          const keywordWords = keyword.split(/\s+/);
          const tagWords = tag.split(/\s+/);
          keywordWords.forEach(kw => {
            if (tagWords.some(tw => tw.includes(kw) || kw.includes(tw))) {
              score += 1;
            }
          });
        });
      });

      return { image: img, score };
    })
    .filter(item => item.score > 0)
    .sort((a, b) => b.score - a.score);

  if (scoredImages.length > 0) {
    const selectedImage = scoredImages[0].image;
    usedImages.add(selectedImage.label);
    return selectedImage;
  }

  // No keyword match found - use random-tagged image as fallback
  console.log('  No keyword match found, using random-tagged image');
  const randomImage = getRandomTaggedImage(images);
  if (randomImage) return randomImage;

  // If no random-tagged images available, fall back to any unused image
  const unusedImages = images.filter(img => !usedImages.has(img.label));
  if (unusedImages.length === 0) return null;

  const selectedImage = unusedImages[Math.floor(Math.random() * unusedImages.length)];
  usedImages.add(selectedImage.label);
  return selectedImage;
}

/**
 * Get icons based on keywords
 * Each icon can be used max 2 times per entire PPT
 * @param {string[]} keywords - Keywords to match
 * @param {number} count - Number of icons needed (default 3)
 * @returns {object[]} - Array of icon objects
 */
function getIcons(keywords, count = 3) {
  // Filter to only icons
  const icons = tagData.filter(item => item.type === "icon");

  // Filter icons that haven't been used 2 times yet
  const availableIcons = icons.filter(icon => {
    const currentCount = iconUsageCount.get(icon.label) || 0;
    return currentCount < 2;
  });

  if (availableIcons.length === 0) return [];

  if (!keywords || keywords.length === 0) {
    // Return random available icons
    const shuffled = [...availableIcons].sort(() => Math.random() - 0.5);
    const selected = shuffled.slice(0, count);

    // Update usage counts
    selected.forEach(icon => {
      const currentCount = iconUsageCount.get(icon.label) || 0;
      iconUsageCount.set(icon.label, currentCount + 1);
    });

    return selected;
  }

  // Normalize keywords
  const normalizedKeywords = keywords.map(k => k.toLowerCase().trim());

  // Score each icon by matching tags
  const scoredIcons = availableIcons
    .map(icon => {
      let score = 0;
      const iconTags = icon.tags.map(t => t.toLowerCase());

      normalizedKeywords.forEach(keyword => {
        iconTags.forEach(tag => {
          if (tag.includes(keyword) || keyword.includes(tag)) {
            score += 2;
          }
          // Partial match
          const keywordWords = keyword.split(/\s+/);
          const tagWords = tag.split(/\s+/);
          keywordWords.forEach(kw => {
            if (tagWords.some(tw => tw.includes(kw) || kw.includes(tw))) {
              score += 1;
            }
          });
        });
      });

      return { icon, score };
    })
    .sort((a, b) => b.score - a.score);

  // Get top matching icons, or fill with random ones
  const result = [];
  const seenLabels = new Set();

  // First add matching icons
  for (const item of scoredIcons) {
    if (result.length >= count) break;
    if (item.score > 0 && !seenLabels.has(item.icon.label)) {
      result.push(item.icon);
      seenLabels.add(item.icon.label);
    }
  }

  // Fill remaining with random available icons
  if (result.length < count) {
    const remaining = availableIcons.filter(i => !seenLabels.has(i.label));
    const shuffled = remaining.sort(() => Math.random() - 0.5);

    for (const icon of shuffled) {
      if (result.length >= count) break;
      result.push(icon);
      seenLabels.add(icon.label);
    }
  }

  // Update usage counts
  result.forEach(icon => {
    const currentCount = iconUsageCount.get(icon.label) || 0;
    iconUsageCount.set(icon.label, currentCount + 1);
  });

  return result;
}

// Export functions globally
window.resetAssetTracking = resetAssetTracking;
window.getBackgroundImage = getBackgroundImage;
window.getIcons = getIcons;
